<div class="container">
    <div class="row row-offcanvas row-offcanvas-right">
        
        <div class="col-xs-12 col-sm-9">
            <p class="pull-right visible-xs">
                <button type="button" class="btn btn-primary btn-xs" data-toggle="offcanvas">Toggle nav</button>
            </p>
            <div class="jumbotron">
                
                <div class="carousel slide" data-ride="carousel">
                    <!-- Indicators -->
                    <ol class="carousel-indicators">
                        <% Dir.entries("source/img/marketing").select {|f| !File.directory? f}.each_with_index do |file, index| %>
                          <% if index == 0 %>
                            <li data-target="#carousel-example-generic active" data-slide-to="<%= index %>"></li>
                          <% else %>
                            <li data-target="#carousel-example-generic" data-slide-to="<%= index %>"></li>
                          <% end %>
                        <% end %>
                    <!-- <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li> -->
                    </ol>
                    <!-- Wrapper for slides -->
                    <div class="carousel-inner" role="listbox">
                        <% Dir.entries("source/img/marketing").select {|f| !File.directory? f}.each_with_index do |file, index| %>
                          <% if index == 0 %><div class="item active"><% else %> <div class="item"><% end %>
                            <img src="img/marketing/<%= file %>" alt="" style="width:848px;height:322px;">
                            <div class="carousel-caption">
                                Flying Moonlight 1
                            </div>
                          </div>
                        <% end %>
                    </div>
                </div>
